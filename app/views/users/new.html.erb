<h2>So you wanna register, huh?</h2>

<%= form_for @user do |f| %>
  <div>
  <%= f.label :username %>
  <%= f.text_field :username %>
  </div>
  <div>
  <%= f.label :email %>
  <%= f.email_field :email %>
  </div>
  <div>
  <%= f.label :password %>
  <%= f.password_field :password %>
  </div>
  <%= f.submit %>
<% end %>
